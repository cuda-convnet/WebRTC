package com.webrtc.arti.common;

/**
 * 外部用户、企业用户（客服）----->人工总机模块
 * 消息动作类型
 */
public interface ActionType
{
	public static final int REQUEST = 1; //请求分配（外部用户）
	public static final int END = 2; //终止分配请求（WCS，外部用户关闭、退出、掉线或长时间无响应）
	public static final int LOGIN = 3; //注册为人工总机（企业用户）
	public static final int LOGOUT = 4; //注销人工总机（企业用户）	
	public static final int FINISH = 5; //完成服务（企业用户）	
}
